{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/v1/Users/{userId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 58750
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/users/{userId}",
      "UpstreamHttpMethod": [ "Get" ],
      "ServiceName": "user_group_api",
      "LoadBalancer": "LeastConnection",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "together",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/v1/activities/{activityId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 52769
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/activities/{activityId}",
      "UpstreamHttpMethod": [ "Get" ],
      "ServiceName": "activity_api",
      "LoadBalancer": "LeastConnection"
    },
    {
      "DownstreamPathTemplate": "/api/v1/activities?pageIndex={pageIndex}&pageSize={pageSize}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 52769
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/activities/list?pageIndex={pageIndex}&pageSize={pageSize}",
      "UpstreamHttpMethod": [ "Get" ],
      "ServiceName": "activity_api",
      "LoadBalancer": "LeastConnection"
    }
